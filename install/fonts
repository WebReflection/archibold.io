if [ "$(which sudo 2> /dev/null)" = "" ]; then
  bash <(curl -s https://archibold.io/install/sudo)
fi

sudo pacman -S --needed --noconfirm \
  ttf-dejavu ttf-liberation \
  ttf-opensans noto-fonts noto-fonts-emoji noto-fonts-extra
